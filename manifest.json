{
  "manifest_version": 3,
  "name": "House Humblr",
  "version": "1.0",
  "description": "Comment on Real Estate Listings.",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://www.zillow.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "logo-icon.png",
      "48": "logo-icon.png",
      "128": "logo-icon.png"
    }
  }
}
